        "selector": {
          "type": "FragmentSelector",
          "value": "xywh=pixel:99.0722427368164,714.2376708984375,2713.8279037475586,3854.5303955078125",
          "conformsTo": "http://www.w3.org/TR/media-frags/"
        }
