      "dc:source": "https://geniza.princeton.edu/sources/190/",
      "target": {
        "source": {
          "id": "https://cudl.lib.cam.ac.uk/iiif/MS-TS-00010-J-00017-00004/canvas/1",
          "type": "Canvas",
