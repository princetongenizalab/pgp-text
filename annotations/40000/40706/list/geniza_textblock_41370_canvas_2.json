      "body": [
        {
          "type": "TextualBody",
          "label": "verso, note",
          "value": "<ol>\n<li>Sitt sends you warmest greetings.</li>\n<li>Zakiyya sends you warmest greetings.</li>\n<li>Everybody in the house sends you warmest greetings.</li>\n</ol>",
          "format": "text/html"
