      "body": [
        {
          "type": "TextualBody",
          "label": "Verso, right margin",
          "value": "<ol>\n<li>I am concerned about his state,</li>\n<li>may God see to his well-being. Please convey condolences</li>\n<li>to my lord the physician in my name.</li>\n</ol>",
          "format": "text/html"
        }
