      "dc:source": "https://geniza.princeton.edu/sources/135/",
      "target": {
        "source": {
          "id": "https://cudl.lib.cam.ac.uk/iiif/MS-TS-00028-00008/canvas/2",
          "type": "Canvas",
          "partOf": {
            "id": "https://geniza.princeton.edu/documents/7590/iiif/manifest/"
