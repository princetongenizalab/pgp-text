      "modified": "2024-12-23T02:25:51.752969+00:00",
      "dc:source": "https://geniza.princeton.edu/sources/99/",
      "target": {
        "source": {
          "id": "https://geniza.princeton.edu/documents/7221/iiif/textblock/15521/canvas/1/",
          "type": "Canvas",
