        "selector": {
          "type": "SvgSelector",
          "value": "<svg><polygon points=\"381.8,3950.1 432.2,496.5 1625.4,454.5 2062.4,362.1 2373.3,437.7 2986.4,377.1 3616.6,444.4 3557.8,5612.2 1322.6,5410.5 2524.5,4588.7 2516.1,4177\"></polygon></svg>"
        }
