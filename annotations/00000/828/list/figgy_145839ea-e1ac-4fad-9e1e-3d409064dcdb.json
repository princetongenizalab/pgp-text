        "source": {
          "id": "https://figgy.princeton.edu/concern/scanned_resources/2ea90a54-d75d-412d-8e6a-d6066fe47a9a/manifest/canvas/145839ea-e1ac-4fad-9e1e-3d409064dcdb",
